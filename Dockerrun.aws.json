{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [{
	"name": "dct-app",
	"image": "357210185381.dkr.ecr.us-east-1.amazonaws.com/wormbase/datomic-curation-tools:0.1",
	"essential": true,
	"memory": 4096,
	"portMappings": [{
	    "hostPort": 3000,
	    "containerPort": 3000
	}],
	"environment": [{
	    "name": "TRACE_DB",
	    "value": "datomic:ddb://us-east-1/WS255/wormbase"
	}, {
	    "name": "TRACE_REQUIRE_LOGIN",
	    "value": "0"
	}]
    }, {
	"name": "dct-http-proxy",
	"image": "357210185381.dkr.ecr.us-east-1.amazonaws.com/wormbase/datomic-curation-tools_nginx-proxy:0.1",
	"essential": true,
	"memory": 128,
	"portMappings": [{
	    "hostPort": 80,
	    "containerPort": 80
	}],
	"links": [
	    "dct-app"
	]
    }]
}
